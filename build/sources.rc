SVN_USER=svnread
SVN_BIN='svn --config-option config:tunnels:ssh="$SSH_BIN -i $SSH_KEY -o UserKnownHostsFile=/dev/null -o StrictHostKeychecking=no"'
kaltura_svn() (chmod 600 $SSH_KEY; eval "$SVN_BIN $@")

